{
  "family": "fragments",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::381492285037:role/LabRole",
  "taskRoleArn": "arn:aws:iam::381492285037:role/LabRole",
  "containerDefinitions": [
    {
      "name": "fragments",
      "image": "REPLACED_BY_GITHUB_ACTIONS",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "LOG_LEVEL", "value": "info" },
        { "name": "NODE_ENV", "value": "production" },
        { "name": "API_URL", "value": "http://localhost:8080" },
        { "name": "AWS_REGION", "value": "us-east-1" },
        { "name": "AWS_S3_BUCKET_NAME", "value": "naveedahmedsyed-fragments" }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
